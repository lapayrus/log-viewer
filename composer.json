{
    "name": "lapayrus/log-viewer",
    "description": "A modern log viewer for Laravel applications",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Suryapal Rao",
            "email": "suryapal.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lapayrus\\LogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lapayrus\\LogViewer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lapayrus\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "log",
        "viewer",
        "log-viewer",
        "logs",
        "lapayrus"
    ],
    "homepage": "https://github.com/lapayrus/log-viewer",
    "minimum-stability": "dev",
    "prefer-stable": true
}